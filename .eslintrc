{
  "extends": "react-app",
  "rules": {
    "jsx-a11y/anchor-is-valid": "off",
    "object-curly-spacing": ["error", "always"],
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ]
  },
  "globals": {
    "gtm": false
  }
}
